@page
@model PrivacyModel
@{
    ViewData["Title"] = "Privacy Policy";
    // TESTING: SCALING
    int n = 1000;
    int count=0;
    long a = 2;
    while(count<n)
    {
        long b = 2;
        int prime = 1;// to check if found a prime
        while(b * b <= a)
        {
            if(a % b == 0)
            {
                prime = 0;
                break;
            }
            b++;
        }
        if(prime > 0)
        {
            count++;
        }
        a++;
    }
    var result = --a;
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy. 1000th prime number: @result</p>
